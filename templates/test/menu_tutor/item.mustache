<div class="list-tutoring-group-item d-flex justify-content-between align-items-center mb-2">
  <div class="d-flex align-items-center">
    <div class="position-relative">
      <img src="{{profileimageurl}}" class="tutoring_profile_image rounded-circle" />
      <a class="btn btn-tutoring-tutor-calendar position-absolute" href="{{globals.config.wwwroot}}/mod/scheduler/view.php?id={{mod_scheduler_id}}&experienceid={{experience.data.id}}">
        <i class="fa fa-calendar"></i>
      </a>
    </div>
    <div class="ml-3">
      <h6 class="mb-0">{{firstname}} {{lastname}}</h6>
      {{#university}}
        <small>{{#str}}tutoring:at_university, local_digitalta{{/str}} {{university}}</small>
      {{/university}}
    </div>
  </div>
</div>
