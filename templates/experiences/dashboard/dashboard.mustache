<div class="container-fluid col-12 col-sm-10 digitalta-experiences">
    <!--Section: Title-->
    <section data-section-name="digitalta-experiences-title" class="mb-4">
        <div class="container-fluid d-flex">
            <div class="row align-items-justify">
                <div class="col-12">
                    <div class="digitalta-subtitle">DigitalTA</div>
                    <h1 class="digitalta-title display-3">{{{title}}}</h1>
                </div>
            </div>
        </div>
    </section>
    <!--Section: Introduction-->
    <section data-section-name="digitalta-experiences-introduction" class="mb-4">
        <div class="container-fluid d-flex text-center">
            <div class="row align-items-center">
                <div class="col-12 text-justify" style="line-height: 2em;">
                    {{#str}} experiences:description, local_digitalta {{/str}}
                </div>
            </div>
        </div>
    </section>
    <!--Section: Experiences-->
    <section data-section-name="digitalta-experiences-list" class="mb-4">
        <div class="container-fluid d-flex">
            {{> local_digitalta/experiences/dashboard/experience-list}}
        </div>
    </section>
</div>