<div class="row">
  <div class="col-md-12">
    <div class="card mb-3">
      <div class="card-body">
        
        <input type="hidden" name="id" id="section-header-id" value="{{sectionheader.id}}">
        <input type="hidden" id="section-header-title" value="{{sectionheader.title}}">
        <input type="hidden" id="section-header-description" value="{{sectionheader.description}}">

        <label class="form-label">{{#str}} ourcases_header_title, local_dta {{/str}}</label>
        <p>{{sectionheader.title}}</p>
        <label class="form-label">{{#str}} ourcases_header_description, local_dta {{/str}}</label>
        <p>{{sectionheader.description}}</p>

        <div class="mt-3">
          <button type="button" class="btn btn-outline-secondary" id="header-to-edit-button" ><i class="fa fa-pencil" aria-hidden="true"></i></button>
        </div>

      </div>
    </div>
  </div>
</div>