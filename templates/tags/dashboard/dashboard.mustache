<div class="container-fluid col-12 col-sm-10 digitalta-themestags">
    <!--Section: Title-->
    <section data-section-name="digitalta-themestags-title">
        <div class="container-fluid d-flex">
            <div class="row align-items-justify">
                <div class="col-12">
                    <div class="digitalta-subtitle">DigitalTA</div>
                    <h1 class="digitalta-title display-3">{{{title}}}</h1>
                </div>
            </div>
        </div>
    </section>
    <!--Section: Introduction-->
    <section data-section-name="digitalta-themestags-introduction">
        <div class="container-fluid d-flex text-center">
            <div class="row align-items-center">
                <div class="col-12 pt-4 text-justify" style="line-height: 2em;">
                    {{#str}} themestags:description, local_digitalta {{/str}}
                </div>
            </div>
        </div>
    </section>
    <!--Section: Themes-->
    <section data-section-name="digitalta-themestags-themes">
        <div class="container-fluid d-flex my-4">
            {{> local_digitalta/tags/dashboard/theme-list}}
        </div>
    </section>
    <!--Section: Tags-->
    <section data-section-name="digitalta-themestags-tags">
        <div class="container-fluid d-flex my-4">
            {{> local_digitalta/tags/dashboard/tag-list}}
        </div>
    </section>
</div>
