<div class="container-fluid col-10 dta-themestags">
    <!--Section: Title-->
    <section data-section-name="dta-themestags-title">
        <div class="container-fluid d-flex">
            <div class="row d-flex align-items-justify">
                <div class="col-12">
                    <div class="dta-subtitle">DigitalTA</div>
                    <h1 class="dta-title display-3">{{{title}}}</h1>
                </div>
            </div>
        </div>
    </section>
    <!--Section: Introduction-->
    <section data-section-name="dta-themestags-introduction">
        <div class="container-fluid d-flex text-center">
            <div class="row d-flex align-items-center">
                <div class="col-12 pt-4 text-justify" style="line-height: 2em;">
                    {{#str}} themestags:description, local_dta {{/str}}
                </div>
            </div>
        </div>
    </section>
    <!--Section: Themes-->
    <section data-section-name="dta-themestags-themes">
        <div class="container-fluid d-flex my-4">
            {{> local_dta/tags/dashboard/theme-list}}
        </div>
    </section>
    <!--Section: Tags-->
    <section data-section-name="dta-themestags-tags">
        <div class="container-fluid d-flex my-4">
            {{> local_dta/tags/dashboard/tag-list}}
        </div>
    </section>
</div>
